<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lifeassistant_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menu_activity.Report_activity"></activity>
        <activity android:name=".menu_activity.ViewPlan_activity" />
        <activity android:name=".menu_activity.NewPlan_activity" />
        <activity android:name=".menu_activity.Planner_activity" />
        <activity android:name=".menu_activity.Invoice_activity" />
        <activity android:name=".menu_activity.Weather_activity" />
        <activity android:name=".menu_activity.Bookkeeping_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>